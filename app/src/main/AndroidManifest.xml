<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rollyglobe.rollyglobe">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MyPage.MyPageEditPasswordActivity"></activity>
        <activity android:name=".MyPage.MyPageEditBirthdayActivity" />
        <activity android:name=".MyPage.MyPageEditGenderActivity" />
        <activity android:name=".MyPage.MyPageEditEmailActivity" />
        <activity android:name=".MyPage.MyPageEditPhoneNumberActivity" />
        <activity android:name=".MyPage.MyPageEditNameActivity" />
        <activity android:name=".MyPage.ProfileEditActivity" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="${googleMapApiKey}" />

        <activity android:name=".InnerContentsActivity" />
        <activity android:name=".SignInActivity">

        </activity>
        <activity android:name=".SignUpActivity" />
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
    </application>

</manifest>